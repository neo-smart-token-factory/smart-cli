{
  "name": "tech-neo-nft",
  "version": "1.0.0",
  "description": "Sistema NFT Manager com IPFS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "server": "node server/index.js",
    "build": "vite build",
    "preview": "vite preview",
    "ipfs:upload": "node scripts/cli-upload.js",
    "ipfs:check": "node scripts/cli-upload.js check",
    "mint": "node scripts/mint.js",
    "mint:draft": "node scripts/mint.js --file=drafts/template.json",
    "nft:query": "node scripts/alchemy-nft-query.js"
  },
  "bin": {
    "ipfs-upload": "./scripts/cli-upload.js"
  },
  "dependencies": {
    "axios": "^1.13.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "ipfs-http-client": "^60.0.1",
    "lucide-react": "^0.294.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^7.1.12"
  }
